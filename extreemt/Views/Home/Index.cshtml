@{
    ViewBag.Title = "Index";
}

<div class="container-fluid">
    <div class="row">
        <div class="box">
            <div class="col-lg-14 text-center">
                <div id="carousel-example-generic" class="carousel slide">
                    <!-- Indicators -->
                    <ol class="carousel-indicators hidden-xs">
                        <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
                        <li data-target="#carousel-example-generic" data-slide-to="1"></li>
                        <li data-target="#carousel-example-generic" data-slide-to="2"></li>
                        <li data-target="#carousel-example-generic" data-slide-to="3"></li>
                    </ol>
                    <!-- Wrapper for slides -->
                    <div class="carousel-inner">
                        <div class="item active">
                            <img class="img-responsive img-full" id="img" src="~/Content/images/are_you_winning_at_life_banner.jpg" alt="">
                        </div>
                        <div class="item">
                            <img class="img-responsive img-full" id="img" src="~/Content/images/2.jpg" alt="">
                        </div>
                        <div class="item">
                            <img class="img-responsive img-full" id="img" src="~/Content/images/Company-culture-dos-and-donts.PNG" alt="">
                        </div>
                        <div class="item">
                            <img class="img-responsive img-full" id="img" src="~/Content/images/deal.jpg" alt="">
                        </div>
                    </div>
                    <!-- Controls -->
                    <a class="left carousel-control" href="#carousel-example-generic" data-slide="prev">
                        <span class="icon-prev"></span>
                    </a>
                    <a class="right carousel-control" href="#carousel-example-generic" data-slide="next">
                        <span class="icon-next"></span>
                    </a>
                </div>
                <h2 class="brand-before">
                    <small>Welcome to</small>
                </h2>
                <h1 class="brand-name">Extreem-T</h1>

            </div>
        </div>
    </div>
</div>

